{
    "name": "olegstan/laravel-rest",
    "description": "Powerful REST for the Laravel framework.",
    "keywords": ["rest", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Oleg Abramov",
            "email": "olegstan@inbox.ru"
        }
    ],
    "scripts": {
        "test": "php vendor/bin/phpunit tests"
    },
    "version": "dev-main",
    "homepage": "https://github.com/olegstan/laravel-rest",
    "require": {
        "php": ">=7.4.0"
    },
    "require-dev": {
        "laravel/laravel": "8.0",
        "phpunit/phpunit": "^9.6",
        "phpunit/php-code-coverage": "^9.2.13"
    },
    "autoload": {
        "psr-4": {
            "LaravelRest\\": "src/"
        }
    },
    "autoload-dev": {

    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelRest\\LaravelRestServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "suggest": {

    },
    "minimum-stability": "stable"
}
