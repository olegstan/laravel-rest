{
    "name": "olegstan/laravel-rest",
    "description": "Powerful REST for the Laravel framework.",
    "keywords": ["rest", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Oleg Abramov",
            "email": "olegstan@inbox.ru"
        }
    ],
    "scripts": {
        "test": "php vendor/bin/phpunit tests"
    },
    "version": "dev-main",
    "homepage": "https://github.com/olegstan/laravel-rest",
    "require": {
        "php": ">=8.0.30"
    },
    "require-dev": {
        "laravel/framework": "^8.0",
        "phpunit/php-code-coverage": "^9.2.13",
        "phpunit/phpunit": "^9.6",
        "rybakit/msgpack": "dev-master",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "LaravelRest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Tests\\Unit\\": "tests/Unit/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelRest\\LaravelRestServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "suggest": {

    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
